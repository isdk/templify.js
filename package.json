{
  "name": "@isdk/templify",
  "description": "ğŸš€ Instantly convert projects into template.",
  "version": "0.5.0",
  "author": "Riceball LEE @snowyu",
  "bin": {
    "templify": "./bin/run.js"
  },
  "bugs": "https://github.com/isdk/templify.js/issues",
  "dependencies": {
    "@isdk/artistic-text": "^0.1.1",
    "@isdk/templify-lib": "^0.5.0",
    "@oclif/core": "^4",
    "@oclif/plugin-autocomplete": "^3.2.34",
    "@oclif/plugin-help": "^6",
    "@oclif/plugin-plugins": "^5",
    "@oclif/plugin-version": "^2.2.32",
    "custom-ability": "^2.0.1"
  },
  "devDependencies": {
    "@eslint/compat": "^1",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.32.0",
    "@oclif/prettier-config": "^0.2.1",
    "@oclif/test": "^4",
    "@types/jest": "^30.0.0",
    "@types/node": "^24",
    "chai": "^5",
    "eslint": "^9",
    "eslint-config-oclif": "^6",
    "eslint-config-prettier": "^10",
    "oclif": "^4.22.3",
    "shx": "^0.4.0",
    "terser": "^5.43.1",
    "tsup": "^8.5.0",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.38.0",
    "vite": "^7.0.6",
    "vitest": "^3.2.4"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "./bin",
    "./dist",
    "./oclif.manifest.json"
  ],
  "homepage": "https://github.com/isdk/templify.js",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "type": "module",
  "oclif": {
    "bin": "templify",
    "dirname": "templify",
    "commands": "./dist/oclif/commands",
    "helpClass": "./dist/oclif/lib/help",
    "hooks": {
      "command_not_found": "./dist/oclif/hooks/command-not-found"
    },
    "plugins": [
      "@oclif/plugin-autocomplete",
      "@oclif/plugin-help",
      "@oclif/plugin-plugins",
      "@oclif/plugin-version"
    ],
    "topicSeparator": " ",
    "topics": {}
  },
  "repository": "isdk/templify.js",
  "scripts": {
    "build": "shx rm -rf dist && tsup",
    "clean": "shx rm -fr oclif.manifest.json dist",
    "lint": "eslint",
    "opack": "oclif pack",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "pnpm run lint",
    "prepack": "NODE_OPTIONS='--trace-warnings --no-warnings=ExperimentalWarning' oclif manifest && oclif readme",
    "release": "pnpm run clean && pnpm run build && pnpm run prepack && git add -f README.md && git ci -m 'docs: update readme' || true && pnpm dlx commit-and-tag-version -s",
    "release.alpha": "pnpm run release -- --prerelease alpha",
    "test": "FORCE_COLOR=1 vitest run",
    "version": "oclif readme && git add README.md"
  },
  "types": "dist/index.d.ts",
  "pnpm": {
    "overrides": {
      "brace-expansion@>=1.0.0 <=1.1.11": ">=1.1.12",
      "brace-expansion@>=2.0.0 <=2.0.1": ">=2.0.2",
      "@eslint/plugin-kit@<0.3.3": ">=0.3.3"
    }
  }
}
